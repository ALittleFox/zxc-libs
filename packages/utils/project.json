{
  "name": "@zxc/utils",
  "root": "packages/utils",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsup",
        "cwd": "packages/utils"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsup --watch",
        "cwd": "packages/utils"
      }
    }
  }
}